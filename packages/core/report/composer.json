{
    "name": "core/report",
    "description": "report",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "crm bizfly",
            "email": "crm.bizfly@vccorp.vn"
        }
    ],
    "require": {
        "php": "8.1.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Core\\Report\\": "src",
            "Core\\Report\\App\\": "src/app"
        },
        "files": [
            "src/app/Helpers/ReportHelper.php"
        ]
    },
    "autoload-dev": {
      "psr-4": {
        "Core\\Report\\Test\\": "tests"
      }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core\\Report\\ReportServiceProvider"
            ]
        }
    }
}
